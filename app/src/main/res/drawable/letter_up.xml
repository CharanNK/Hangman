<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
<item android:state_enabled="true" >
    <shape android:shape="rectangle" android:padding="5dp" xmlns:android="http://schemas.android.com/apk/res/android">
         <solid android:color="#00f2a5"/>
         <corners android:topRightRadius="25dp" android:topLeftRadius="25dp" android:bottomRightRadius="25dp" android:bottomLeftRadius="25dp"/>
          <stroke android:color="#4e342e" android:width="2dp"/>
    </shape>
    <item android:state_enabled="false" >
        <shape android:shape="rectangle" android:padding="5dp" xmlns:android="http://schemas.android.com/apk/res/android">
            <solid android:color="#00f2a5"/>
            <corners android:topRightRadius="25dp" android:topLeftRadius="25dp" android:bottomRightRadius="25dp" android:bottomLeftRadius="25dp"/>
            <stroke android:color="#4e342e" android:width="2dp"/>
        </shape>
    </item> <!-- disabled -->
</item>
</selector>
